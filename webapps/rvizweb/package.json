  "bugs": {
    "url": "https://github.com/osrf/rvizweb/issues"
  },
  "homepage": "https://github.com/osrf/rvizweb#readme",
  "devDependencies": {
    "bower": "^1.8.14",
    "polymer-cli": "^1.9.11"
  }
}
